<h1>All the interesting articles we have</h1>
<div class="menu">
  <i class="fa-solid fa-square-plus"></i>
  <%= link_to "Add a new article", new_article_path%>
</div>
<div>
<% @articles.each do |article| %>
  <div class="article-card">
    <h2><%= link_to article.title, article %></h2>
    <p><%= article.content %></p>
  </div>
<% end %>
</div>
